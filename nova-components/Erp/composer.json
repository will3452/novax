{
    "name": "elezerk/erp",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Elezerk\\Erp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elezerk\\Erp\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
