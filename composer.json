{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./repos"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaPermissionsField"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "coreproc/nova-permissions-field": "*",
        "digital-creative/nova-filepond": "^0.2.13",
        "epartment/nova-dependency-container": "^1.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laraning/nova-time-field": "^1.2",
        "laravel/framework": "^8.65",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.12",
        "laravel/tinker": "^2.5",
        "laravelwebdev/responsive": "^1.0",
        "maatwebsite/excel": "^3.1",
        "optimistdigital/nova-multiselect-field": "^2.3",
        "optimistdigital/nova-settings": "^3.2",
        "richardkeep/nova-timenow": "^1.0",
        "runlinenl/nova-profile-tool": "^1.1",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-permission": "^5.3",
        "spatie/nova-backup-tool": "^4.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
